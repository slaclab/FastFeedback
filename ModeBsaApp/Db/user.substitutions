# Example substitutions file

file "db/circle.db" {
    { user = "tanisha" }
}

file "db/dbExample1.db" {
    { user = "tanisha" }
}

file db/dbExample2.db {
    pattern { user, no, scan }
        { "tanisha", 1, "1 second" }
        { "tanisha", 2, "2 second" }
        { "tanisha", 3, "5 second" }
}
