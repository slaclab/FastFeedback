# define all loops here
file fbckLoop.db {
  pattern { AR,  LP   ,  LN          }
          { FB01, "TR01"  , "LaunchLoop1"   }
          { FB01, "TR02"  , "LaunchLoop2"   }
          { FB01, "TR03"  , "LaunchLoop3"   }
          { FB01, "TR04"  , "LaunchLoop4"   }
          { FB01, "TR05"  , "LaunchLoop5"   }
# Every IOC will have two spares for on the fly general loop configurations 
          { FB01, "GN01"  , "General1"       }
          { FB01, "GN02"  , "General2"       }   
# Every IOC will have one longitudinal loop configuration
          { FB01, "LG01"  , "Longitudinal"   }   
}
file fbckIoc.db {
  pattern { AR }
          { FB01 }
} 
